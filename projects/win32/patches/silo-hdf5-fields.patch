From 3103c01189b76ea6406e5b964748b0ba7e5d4a08 Mon Sep 17 00:00:00 2001
From: Ryan Krattiger <ryan.krattiger@kitware.com>
Date: Wed, 19 Apr 2023 10:45:59 -0500
Subject: [PATCH] HDF5 add new fields for H5FD_class_t

---
 src/hdf5_drv/H5FDsilo.c | 6 ++++++
 1 file changed, 6 insertions(+)

diff --git a/src/hdf5_drv/H5FDsilo.c b/src/hdf5_drv/H5FDsilo.c
index 74594c1b..c36b9b3d 100644
--- a/src/hdf5_drv/H5FDsilo.c
+++ b/src/hdf5_drv/H5FDsilo.c
@@ -585,6 +585,12 @@ static const H5FD_class_t H5FD_silo_g = {
     H5FD_silo_get_handle,                       /*get_handle            */
     H5FD_silo_read,		                /*read			*/
     H5FD_silo_write,		                /*write			*/
+#if HDF5_VERSION_GE(1,13,3) && defined(H5FD_CLASS_VERSION)
+    NULL,                           /*read_vector*/
+    NULL,                           /*write_vector*/
+    NULL,                           /*read_selection*/
+    NULL,                           /*write_selection*/
+#endif
     NULL,		                	/*flush			*/
     H5FD_silo_truncate,				/*truncate		*/
     NULL,                                       /*lock                  */
